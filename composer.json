{
  "name": "paggi/sdk",
  "homepage": "https://docs.paggi.com",
  "description": "Paggi PHP SDK - Ecommerce",
  "type": "library",
  "keywords": [
    "paggi.com",
    "paggi",
    "payments",
    "pagamentos",
    "payments brazil",
    "pagamento brasil"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Paggi IT Team",
    "email": "suporteintegracoes@paggi.com"
  }],
  "require": {
    "doctrine/inflector": "^1.1",
    "lcobucci/jwt": "3.2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "3.*",
    "guzzlehttp/guzzle": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Paggi\\SDK\\": "sdk/concretes/",
      "Paggi\\SDK\\Traits\\": "skd/Traits/",
      "Paggi\\SDK\\Interfaces\\": "sdk/interfaces/",
      "Paggi\\Tests\\": "tests/"
